{
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
  "release-search-depth": 400,
  "group-pull-request-title-pattern": "chore${scope}: Release Multiple ${component} v${version}",
  "pull-request-title-pattern": "chore(${component}): Release v${version} - ${scope}",
  "packages": {
    ".": {
      "tag-separator": "@",
      "include-v-in-tag": true,
      "changelog-path": "CHANGELOG.md",
      "release-type": "simple",
      "bump-minor-pre-major": false,
      "bump-patch-for-minor-pre-major": false,
      "draft": false,
      "prerelease": false,
      "include-component-in-tag": false,
      "extra-files": [
        "examples/shared-workflows/codeowners.yml",
        "examples/shared-workflows/pr-controls.yml",
        "examples/shared-workflows/release.yml",
        "examples/shared-workflows/stale.yml",
        "examples/shared-workflows/R-lang-build.yml",
        "examples/shared-workflows/R-lang-deploy.yml",
        "examples/shared-workflows/R-lang-unit-tests.yml",
        "examples/shared-workflows/SSDT-build.yml",
        "examples/shared-workflows/SSDT-deploy.yml",
        "examples/shared-workflows/SSDT-unit-tests.yml",
        "examples/shared-workflows/SSIS-build.yml",
        "examples/shared-workflows/SSIS-deploy.yml",
        "examples/shared-workflows/SSIS-unit-tests.yml",
        "examples/shared-workflows/dotnet-build.yml",
        "examples/shared-workflows/dotnet-deploy.yml",
        "examples/shared-workflows/dotnet-unit-tests.yml",
        "examples/shared-workflows/go-build.yml",
        "examples/shared-workflows/go-deploy.yml",
        "examples/shared-workflows/go-unit-tests.yml",
        "examples/shared-workflows/java-build.yml",
        "examples/shared-workflows/java-deploy.yml",
        "examples/shared-workflows/java-unit-tests.yml",
        "examples/shared-workflows/nodejs-build.yml",
        "examples/shared-workflows/nodejs-deploy.yml",
        "examples/shared-workflows/nodejs-unit-tests.yml",
        "examples/shared-workflows/php-build.yml",
        "examples/shared-workflows/php-deploy.yml",
        "examples/shared-workflows/php-unit-tests.yml",
        "examples/shared-workflows/python-build.yml",
