{
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
  "release-search-depth": 400,
  "group-pull-request-title-pattern": "chore${scope}: Release ${component} v${version}",
  "pull-request-title-pattern": "chore${scope}: Release ${component} v${version}",
  "packages": {
    ".": {
      "pull-request-title-pattern": "chore${scope}: Release v${version}",
      "tag-separator": "@",
      "include-v-in-tag": true,
      "changelog-path": "CHANGELOG.md",
      "release-type": "simple",
      "bump-minor-pre-major": false,
      "bump-patch-for-minor-pre-major": false,
      "draft": false,
      "prerelease": false,
      "include-component-in-tag": false,
      "extra-files": [
        ".github/workflows/codeowners.yml",
        ".github/workflows/pr-controls.yml",
        ".github/workflows/release.yml",
        ".github/workflows/stale.yml",
        "standardized-workflows/all/**/*"
      ]
    },
    "standardized-workflows/dotnet": {
      "pull-request-title-pattern": "chore${scope}: Release dotnet v${version}",
      "tag-separator": "@",
      "include-v-in-tag": true,
      "changelog-path": "standardized-workflows/dotnet/CHANGELOG.md",
      "release-type": "simple",
      "component": "dotnet",
      "include-component-in-tag": true,
      "extra-files": [
        "standardized-workflows/all/**/*",
        "standardized-workflows/dotnet/**/*",
        ".github/workflows/build-dotnet.yml",
        ".github/workflows/deploy-dotnet.yml",
        ".github/workflows/units-dotnet.yml"
      ]
    },
    "standardized-workflows/python": {
      "pull-request-title-pattern": "chore${scope}: Release python v${version}",
